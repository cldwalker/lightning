---
generated_file: /Users/bozo/.bash/completion/lightning_completions
ignore_paths:
    - .DS_Store
    - .git
#These entries should each map a unique key to a list of directories. When autocompleting, each
#of the directories will provide the immediate files and directories under them as possible
#completions. The names of these paths should only be alphanumeric characters ie no special characters.
bolts:
    app:
      alias: a
      commands:
      - shell_command: open -a
        name: oa
      paths:
        - /Applications/*.app
        - /Applications/Utilities/*.app
    ruby19_files:
      alias: r9
      commands:
      - vim
      - mate
      - less
      paths:
      - /Users/bozo/.rvm/ruby-1.9.1-p243/lib/ruby/**/*.{rb,bundle,so,c}
    ruby_files:
      alias: r
      commands:
      - vim
      - mate
      - less
      paths: 
      #picked from $LOAD_PATH
      - /Library/Ruby/Site/1.8/**/*.{rb,bundle,so}
      - /System/Library/Frameworks/Ruby.framework/Versions/1.8/usr/lib/ruby/1.8/**/*.{rb,bundle,so,c}
    ruby_dirs:
      alias : rd
      commands:
      - shell_command: cd
        aliases:
          r1: /System/Library/Frameworks/Ruby.framework/Versions/1.8/usr/lib/ruby/1.8
      paths:
       - /Library/Ruby/Site/1.8/**/
       - /System/Library/Frameworks/Ruby.framework/Versions/1.8/usr/lib/ruby/1.8/**/
    rb:
      description : base directory of a ruby gem project
      paths:
        - **/*.rb
        - bin/**
      commands:
      - vim
      - less
    code:
      alias: c
      commands:
      - mate
      - name : c
        shell_command : cd
        description : my code directories
        aliases:
          mc: /Users/bozo/code/gems/my_core
      paths: 
      - /Users/bozo/code/gems/*
      - /Users/bozo/code/repo/*
      - /Users/bozo/code/repo/priv/*
      - /Users/bozo/code/tds/gems/*
      - /Users/bozo/code/fork/*
    notes:
      paths:
      - /Users/bozo/docs/notes/**/*.otl
      alias: n
      commands:
      - vim
    ror:
      description: open files when in the base directory of a rails project
      paths:
      #- config/**/*.rb
      - app/**/*.{rb,erb,rhtml}
      - **/*.rb
      #- lib/**/*.rb
      #- test/**/*.rb
      #- spec/**/*.rb
      commands:
      - vim
    # to obtain your own paths to your ruby gems:
    # `gem environment path`.split(":").map {|e| e +"/gems/*" }
    gem:
      alias: g
      paths:
      - /Users/bozo/.gem/ruby/1.8/gems/*
      - /Library/Ruby/Gems/1.8/gems/*
      - /System/Library/Frameworks/Ruby.framework/Versions/1.8/usr/lib/ruby/gems/1.8/gems/*
      commands:
      - name : pb-g
        shell_command : echo -n
        add_to_command: |pbcopy
      - mate
      - grep
      - shell_command : cd
        aliases:
          g1: /Library/Ruby/Gems/1.8/gems
          g2: /Users/bozo/.gem/ruby/1.8/gems
          g3: /System/Library/Frameworks/Ruby.framework/Versions/1.8/usr/lib/ruby/gems/1.8/gems
    gem_rdoc:
      alias: gd
      paths:
      - /Users/bozo/.gem/ruby/1.8/doc/*
      - /Library/Ruby/Gems/1.8/doc/*
      - /System/Library/Frameworks/Ruby.framework/Versions/1.8/usr/lib/ruby/gems/1.8/doc/*
      commands:
      - shell_command : open
        description : open gem's doc in browser
        post_path : /rdoc/index.html
      - shell_command : ri -d
        description : access ri documentation limited to this gem
        post_path : /ri
        add_to_command: --no-standard-docs ${@:2}
        completion_regex: ^\S+
shell_commands:
  mate: m
  vim: v
  open: o
  less: l
  grep: g